#tag Events ccOKCancel1
	#tag Event
		Sub CancelClicked()
		  bSave = false
		  self.visible = false
		End Sub
	#tag EndEvent
	#tag Event
		Sub OKClicked()
		  if validate = false then return
		  
		  save
		  
		  bSave = true
		  
		  self.visible = false
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
