#tag WindowCode
	#tag Method, Flags = &h0
		Function Display(oteam as Data.t_team) As boolean
		  moTeam = oTeam
		  
		  Load
		  
		  self.ShowModal
		  
		  
		  Return bSave
		End Function
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub Load()
		  txtKey.text = moTeam.sKey
		  txtName.text= moTeam.sName
		  txtNickName.text= moTeam.sNickName
		  txtTeamNumber.text= moTeam.sTeam_Number
		  txtLocality.text= moTeam.sLocality
		  txtRegion.text= moTeam.sRegion
		  txtRookieyear.text = str(moTeam.iRookie_Year)
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub Save()
		  moTeam.sKey = txtKey.text
		  moTeam.sName = txtName.text
		  moTeam.sNickName = txtNickName.text
		  moTeam.sTeam_Number = txtTeamNumber.text
		  moTeam.sLocality = txtLocality.text
		  moTeam.sRegion = txtRegion.text
		  moTeam.iRookie_Year = txtRookieyear.text.val
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Function Validate() As boolean
		  
		  
		  return true
		  
		End Function
	#tag EndMethod


