#tag WindowCode
	#tag Event
		Function MouseDown(X As Integer, Y As Integer) As Boolean
		  Timer1.mode = timer.ModeOff
		  
		  if bAbout  then
		    self.close
		  else
		    DoSetupWindow
		  end
		End Function
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub Display()
		  self.show
		  
		  timer1.mode = timer1.modeoff
		  bAbout = true
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub DoSetupWindow()
		  
		  winstartup.show
		  
		  self.close
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
		bAbout As boolean
	#tag EndProperty


#tag EndWindowCode

#tag Events Timer1
	#tag Event
		Sub Action()
		  me.mode = timer.modeoff
		  
		  if bAbout = false then
		    DoSetupWindow
		  end
		  
		End Sub
	#tag EndEvent
#tag EndEvents
