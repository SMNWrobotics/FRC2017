#tag WindowCode
	#tag Event
		Sub DropObject(obj As DragItem, action As Integer)
		  if obj.FolderItemAvailable = false then return
		  
		  dim f as folderitem = obj.FolderItem
		  
		  dim w as new winImport
		  
		  w.display(f)
		End Sub
	#tag EndEvent

	#tag Event
		Sub Open()
		  Init
		  
		  me.AcceptFileDrop(FileTypes1.SQLite)
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub Init()
		  //Check to see if we have any events, teams, matches
		  
		  dim sKey as string = preferences.StringValue("EventKey")
		  
		  sKey = "2017mokc"
		  
		  if sKey <> "" then
		    moEvent = Data.T_event.FindByKey(sKey)
		  end
		  
		  if moEvent = nil or moEvent.ID = 0 then
		    lblEvent.text = "2017 KC Tournament not found."
		    lblEvent.TextColor = &cFF0000
		    bbExportMatchData.Enabled = false
		    bbImportMatchData.Enabled = true
		    bbScout.Enabled = false
		    bbList.Enabled = false
		    lblCustomized.text = kLabel.replace("^1", "unknown")
		    bbReporting.Enabled = false
		    bbRobot.Enabled = false
		  else
		    lblEvent.text = "Event: " + moEvent.sShort_Name + " at " + moEvent.sLocation
		    lblEvent.TextColor = &c00800000
		    bbExportMatchData.Enabled = true
		    bbImportMatchData.Enabled = true
		    bbScout.Enabled = true
		    bbList.Enabled = true
		    lblCustomized.text = kLabel.replace("^1", moEvent.sShort_Name + " at " + moEvent.sLocation)
		    bbReporting.Enabled = true
		    bbRobot.Enabled = true
		  end
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
		moEvent As Data.t_event
	#tag EndProperty


	#tag Constant, Name = kLabel, Type = String, Dynamic = False, Default = \"Scout a match at ^1.  Keep track of one robot in a match and score how it does.", Scope = Public
	#tag EndConstant


